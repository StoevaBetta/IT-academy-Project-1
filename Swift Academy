import java.util.Scanner;

/**
 * Created by gerganastoeva on 10/10/17.
 */
public class swift1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Please, enter N = ");
        int n = s. nextInt();

        for (int i = 1; i <= n; i++) {
            if (i % 3 == 0);
            System.out.println(i);
        }
    }
}
