import java.util.Scanner;

/**
 * Created by gerganastoeva on 10/10/17.
 */
public class swift1 {
    public static void main(String[] args) {
        int[] myArray = new int[10];

        for (int i = 0; i < myArray.length; i++){
            myArray[i] = i * 2;
        }

        for (int i = 0; i < myArray.length; i++){
            System.out.println("Element = " + myArray[i]);
        }

    }
}


